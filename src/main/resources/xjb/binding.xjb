<bindings
		xmlns="https://jakarta.ee/xml/ns/jaxb"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
		xmlns:annox="urn:jaxb.jvnet.org:annox"
		xmlns:jaxws="https://jakarta.ee/xml/ns/jaxws"
		xmlns:s="http://www.codecentric.de/namespace/weatherservice/datatypes" 
		extensionBindingPrefixes="xjc annox"
		version="3.0">
	<globalBindings>
		<serializable uid="1" />
		<xjc:simple />
				<xjc:javaType name="java.util.Calendar" xmlType="xs:date" adapter="my.example.jaxb.adapter.DateAsStringToCalendarXmlAdapter"/>
		
			</globalBindings>
</bindings>